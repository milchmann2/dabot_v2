{
  "name": "dabot_v2",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "dependencies": {
    "@types/node": "^14.0.27",
    "chalk": "^4.1.0",
    "express": "^4.17.1",
    "jest": "^26.4.0",
    "open": "^7.1.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.7",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "eslint": "^7.7.0",
    "nodemon": "^2.0.4",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "scripts": {
    "prestart": "",
    "start": "node --inspect=5858 -r ts-node/register ./src/srcServer.ts",
    "dev": "nodemon",
    "build": "tsc",
    "localtunnel": "lt --port 3000",
    "share": "npm-run-all --parallel start localtunnel",
    "lint": "eslint . --ext .ts",
    "lint:watch": "npm run lint -- --watch",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/milchmann2/dabot_v2.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/milchmann2/dabot_v2/issues"
  },
  "homepage": "https://github.com/milchmann2/dabot_v2#readme",
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm start",
    "ext": "ts",
    "restartable": "rs",
    "delay": "2500",
    "env": {
      "NODE_ENV": "development",
      "PORT": 3000
    }
  }
